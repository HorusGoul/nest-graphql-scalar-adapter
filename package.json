{
  "name": "nest-graphql-scalar-adapter",
  "description": "A better way to import external GraphQL scalars into your Nest.js projects.",
  "keywords": [
    "nest",
    "node",
    "graphql",
    "nestjs",
    "scalar",
    "adapter",
    "typescript"
  ],
  "version": "1.1.1",
  "license": "MIT",
  "author": "Horus Lugo <horusgoul@gmail.com>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/nest-graphql-scalar-adapter.esm.js",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --target=node",
    "build": "tsdx build --target=node",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build --target=node"
  },
  "peerDependencies": {
    "@nestjs/graphql": "^7.0.0 || ^8.0.0",
    "tslib": "^2.0.1"
  },
  "husky": {
