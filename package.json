    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/core": "^8.0.6",
    "@nestjs/graphql": "^8.0.2",
    "@nestjs/testing": "^8.0.6",
    "apollo-server-core": "^3.1.2",
    "graphql": "^15.5.1",
    "graphql-type-json": "^0.3.2",
    "husky": "^4.3.0",
    "reflect-metadata": "^0.1.13",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "prettier": "^2.1.2"
  }
}
